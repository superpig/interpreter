{
  "name": "interpreter",
  "version": "1.0.0",
  "description": "a simple Pascal interpreter with Typescript language",
  "main": "index.js",
  "scripts": {
    "start": "name=__main__ ts-node-dev --respawn --transpileOnly src/part7/ex2.ts",
    "debug": "name=__main__ ts-node-dev --inspect --respawn --transpileOnly src/part7/calc.ts",
    "style": "run-s style:**",
    "style:prettier": "prettier --write \"src/**/*.ts\"",
    "sytle:lint": "eslint src --fix --ext .ts,.tsx",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/superpig/interpreter.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn style",
      "commit-msg": "validate-commit-msg"
    }
  },
  "keywords": [
    "Pascal",
    "interpreter"
  ],
  "author": "superpig <940578704@qq.com> (https://github.com/superpig)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/superpig/interpreter/issues"
  },
  "homepage": "https://github.com/superpig/interpreter#readme",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.7.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "readline-sync": "^1.4.10",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.49",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.3",
    "validate-commit-msg": "^2.14.0"
  },
  "dependencies": {}
}
